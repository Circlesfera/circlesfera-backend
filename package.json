{
  "name": "@circlesfera/backend",
  "version": "0.1.0",
  "description": "Backend de CircleSfera basado en Clean Architecture y Node.js.",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env --tsconfig ./tsconfig.json src/app.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/app.js",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/lib-storage": "^3.645.0",
    "@socket.io/redis-adapter": "^8.2.0",
    "@typegoose/typegoose": "^12.4.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "argon2": "^0.31.2",
    "bullmq": "^5.6.5",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.0.0",
    "express-rate-limit": "^7.1.5",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.1",
    "multer": "^1.4.5-lts.1",
    "pino": "^10.1.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.16.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.4.1",
    "@typescript-eslint/parser": "^8.4.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.8",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.3.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.16.3",
    "typescript": "^5.6.2"
  }
}
